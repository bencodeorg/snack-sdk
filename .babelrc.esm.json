{
  "presets": [
    ["env", {
      "modules": false,
      "targets": {
        "browsers": ["last 2 versions", "ie >= 10"]
      }
    }]
  ],
  "plugins": [
    "syntax-dynamic-import",
    "transform-flow-strip-types",
    "transform-class-properties",
    "transform-object-rest-spread",
    "transform-runtime",
    ["transform-runtime", { "polyfill": false }]
  ],
  "sourceMaps": true,
  "babelrc": false
}
